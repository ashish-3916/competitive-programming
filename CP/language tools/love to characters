#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    string ss;
    cin>>ss;
    int a=0, s=0,p=0;
     for(int i=0;i<n;i++)
     {
         if(ss[i]=='a')a++;
          if(ss[i]=='s')s++;
          if(ss[i]=='p')p++;
     }
    cout<<a<<" "<<s<<" "<<p<< endl;
    
	return 0;
}
